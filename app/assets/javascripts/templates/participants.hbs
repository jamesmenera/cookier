<div class="row">
  <div class="columns four">
    <h4>Participants</h4>
    <ul class="no-bullet">
      {{#each participant in controller}}
        {{view Gsc.ParticipantInListView contentBinding="participant"}}
      {{/each}}
    </ul>

    {{#linkTo participants class="button small radius mbm"}}Add Participant{{/linkTo}}
  </div>
  <div class="columns eight">
    {{outlet}}
  </div>
</div>